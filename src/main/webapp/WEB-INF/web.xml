<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SigridServlet</servlet-name>
  	<display-name>SigridServlet</display-name>
  	<description></description>
  	<servlet-class>web.SigridServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RomainServlet</servlet-name>
  	<display-name>RomainServlet</display-name>
  	<description></description>
  	<servlet-class>web.RomainServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SigridServlet</servlet-name>
  	<url-pattern>/Sigrid</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RomainServlet</servlet-name>
  	<url-pattern>/Romain</url-pattern>
  </servlet-mapping>
</web-app>
